<!DOCTYPE html>
<html lang="en">

<% const headTitle = (!titles) ? "Geen titel" : titles["short-title"]._ %>
<%- include partials/head.ejs %>

<body>
  <%- include partials/header-nav.ejs %>
  <main id="l-detail">
    <img src="data:image/png;base64," alt="Foto niet verkrijgbaar">

    <aside class="detail-aside">
      <ul class="detail-data">
        <li data-prefix="Titel"><%= (!titles) ? "Geen titel" : titles["short-title"]._ %></li>
        <li data-prefix="Auteur"><%= (!authors) ? "Geen auteurs" : authors["main-author"]._ %></li>
        <li data-prefix="Formaat"><%= (!formats) ? "Geen formaten" : formats.format._ %></li>
        <li data-prefix="Publicatie Datum"><%= (!publication) ? "Geen publicatie datum" : publication.year._ %></li>
      </ul>
      <ul class="detail-tags">
        <% if (subjects) { %>
          <% if (!subjects["topical-subject"].length) { %>
            <li><%= subjects["topical-subject"]._ %></li>
          <% } else { %>
            <% subjects["topical-subject"].forEach(subj => { %>
              <li><%= subj._ %></li>
            <% }) %>
          <% } %>
        <% } %>
      </ul>
      <p><%= (!summaries) ? "Geen samenvatting" : summaries.summary._ %></p>
    </aside>
  </main>
</body>
</html>
